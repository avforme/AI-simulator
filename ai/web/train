# PURPOSE.

AIPLANNER_HOME=${AIPLANNER_HOME:-$HOME/aiplanner}

source $AIPLANNER_HOME/ai/helpers.bash

evaluate single-spias-timesteps_1m test "-c $SINGLE_EVAL_FILE --master-p-tax-free=5e5 --master-nominal-spias --eval-num-timesteps=10000"
exit

evaluate single-spias-timesteps_1m age_start50-defined_benefits16e3-tax_free5e5 "-c $SINGLE_EVAL_FILE --master-p-tax-free=5e5 --master-nominal-spias"
evaluate single-spias-timesteps_1m age_start50-defined_benefits16e3-tax_deferred5e5 "-c $SINGLE_EVAL_FILE --master-p-tax-deferred=5e5 --master-nominal-spias"
evaluate single-spias-timesteps_1m age_start50-defined_benefits16e3-taxable_stocks5e5 "-c $SINGLE_EVAL_FILE --master-p-taxable-stocks=5e5 --master-nominal-spias"
evaluate single-spias-timesteps_1m retired65-defined_benefits16e3-tax_free5e5 "-c $SINGLE_EVAL_FILE --master-age-start=65 --master-p-tax-free=5e5 --master-nominal-spias"
evaluate single-spias-timesteps_500k age_start50-defined_benefits16e3-tax_free5e5 "-c $SINGLE_EVAL_FILE --master-p-tax-free=5e5 --master-nominal-spias"
evaluate single-spias-timesteps_500k age_start50-defined_benefits16e3-tax_deferred5e5 "-c $SINGLE_EVAL_FILE --master-p-tax-deferred=5e5 --master-nominal-spias"
evaluate single-spias-timesteps_500k age_start50-defined_benefits16e3-taxable_stocks5e5 "-c $SINGLE_EVAL_FILE --master-p-taxable-stocks=5e5 --master-nominal-spias"
evaluate single-spias-timesteps_500k retired65-defined_benefits16e3-tax_free5e5 "-c $SINGLE_EVAL_FILE --master-age-start=65 --master-p-tax-free=5e5 --master-nominal-spias"
wait
exit

train single-spias-timesteps_1m '--master-nominal-spias'
train single-spias-timesteps_500k '--master-nominal-spias --train-single-num-timesteps=500000'
exit

# Variable scenarios.

train single-no_spias
train single-spias '--master-nominal-spias'

train couple-no_spias '--master-sex2=male'
train couple-spias '--master-sex2=male --master-nominal-spias'
